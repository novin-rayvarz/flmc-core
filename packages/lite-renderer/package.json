{
  "name": "flmc-lite-renderer",
  "version": "1.0.33",
  "description": "",
  "author": "SahandAkbarzadeh",
  "license": "MIT",
  "repository": "rayvarz-open-source/FLMC-core",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "prepare-for-publish": "tsc",
    "start": "rollup -c -w",
    "deploy": "gh-pages -d example/build",
    "version:patch": "npm version patch && git add . && git commit -m \"Bump Version\"",
    "version:major": "npm version major && git add . && git commit -m \"Bump Version\"",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" --print-width 120",
    "publish-to-npm": "del .\\build\\ && npm run prepare-for-publish && npm run version:patch && npm publish"
  },
  "dependencies": {
    "notistack": "^0.8.9",
    "react-number-format": "^4.0.8",
    "react-text-mask": "^5.4.3",
    "rxjs": "^6.5.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.8.5",
    "react-dom": "^15.0.0 || ^16.8.5"
  },
  "devDependencies": {
    "material-table": "^1.40.0",
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "prettier": "1.18.2",
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-scripts-ts": "^4.0.8",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-url": "^1.4.0",
    "tslib": "^1.9.3",
    "typescript": "^3.5.1"
  },
  "files": [
    "build/**/*"
  ]
}
